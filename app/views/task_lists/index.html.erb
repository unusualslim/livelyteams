<%= link_to 'New Task List', new_task_list_path, :class => "btn btn-primary" %>

<table class="table table-hover">
  <thead>
    <tr>
      <td scope="col">Title</td>
      <td scope="col">Updated</td>
    </thead>
    <tbody>
      <tr>  
        <% @task_lists.each do |tl| %>
        <tr>
        <td><%= link_to tl.name, task_list_path(tl) %></td>
        <td><%= tl.updated_at %></td>
        </tr>
        <% end %>
    </tbody>
  </thead>
</table>

